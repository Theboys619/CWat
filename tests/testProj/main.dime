extern func void print_str(x: str);

class Test {
  i32 x;
  str wows;

  func Test(y: i32, f: str) {
    this.x = y
    this.wows = f;
  }

  func void print() {
    print_str(this.wows);
  }
};

export func Test main(x: i32) {
  let i32 y = 0;

  while (y < x) {
    print_str("Hello, I am in a loop!\n")
    y++;
  }

  let Test z = Test(5, "Nice!!!");
  z.print();

  return z;
}